  "scripts": {
    "generate-build-version": "node generate-build-version",
    "start": "node scripts/start",
    "build": "npm run generate-build-version &&  node scripts/build",
    "postbuild": "cd D:\\Crayond-workspace\\nac-ecom-ui\\build\\static\\js && gzip *.js && cd D:\\Crayond-workspace\\nac-ecom-ui\\build\\static\\css &&  gzip *.css",
    "test": "node scripts/test",
    "serve": "npm run build && serve -S build",
    "deploy_staging": "npm run build && git add . && git commit -m `Updated to the latest version` && git push origin master",
    "deploy_prod": "npm run build && git add . && git commit -m `Updated to the latest version` && git push origin prod",
    "lint": "eslint src/*.js",
    "hint": "hint http://localhost:8080",
    "generate-report": "lighthouse --chrome-flags=\"--headless --no-sandbox\" http://localhost:8080 --output html --output-path ./report.html",
    "pm2-serve": "pm2 delete nac-ecom || pm2 serve --spa build --name nac-ecom",
    "pm2-rm": "pm2 delete nac-ecom",
    "sb": "start-storybook"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@craco/craco": "5.2.4",
    "@storybook/addon-actions": "^5.1.11",
    "@storybook/react": "5.1.10",
    "hint": "^5.2.0",
    "nodemon": "1.19.1",
    "pm2": "3.5.1",
    "serve": "11.0.2",
    "storybook-addon-material-ui": "^0.9.0-alpha.20",
    "storybook-react-router": "^1.0.5",
    "sw-precache-webpack-plugin": "^0.11.5"
  },
  "cracoConfig": "craco-config.js",
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jest-environment-jsdom-fourteen",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [
      "E:\\jayasoorya\\stylori-ui\\src"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  }
}
